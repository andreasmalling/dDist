public class main {

    public static void main(String[] args){
        RSAImpl rsa = new RSAImpl();
        System.out.println(rsa.keyGen(16).toString());
        System.out.println("Number to en/decrypt is 123456");
        rsa.encrypt("12412");
        rsa.decrypt();
    }

}
